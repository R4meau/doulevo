{
  "name": "doulevo",
  "version": "0.0.2",
  "description": "Prototype version of Doulevo. Don't use in production.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "start": "ts-node src/index.ts create test-project --force --local-plugin=c:/projects/doulevo/plugins/nodejs",
    "start:dev": "nodemon",
    "c": "npm run clean",
    "clean": "rm -rf build/*",
    "b": "npm run build",
    "build": "tsc --incremental",
    "bw": "npm run build:watch",
    "build:watch": "tsc  --incremental --watch",
    "prepublishOnly": "npm test && npm run clean && tsc",
    "t": "npm run test",
    "test": "jest",
    "tw": "npm run test:watch",
    "test:watch": "jest --watch",
    "build-exe": "npm run test && npm run clean && npx tsc && npx nexe build/index.js"
  },
  "bin": {
    "doulevo": "./bin/cli"
  },
  "keywords": [],
  "author": "ashley@codecapers.com.au",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.20",
    "@types/minimist": "^1.2.1",
    "@types/node": "^14.14.25",
    "jest": "^26.6.3",
    "nexe": "^4.0.0-beta.18",
    "nodemon": "^2.0.7",
    "source-map-support": "0.5.19",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@codecapers/fusion": "^1.0.5",
    "fs-extra": "^9.1.0",
    "inflate-template": "^1.1.9",
    "inquirer": "^8.0.0",
    "minimist": "^1.2.5"
  }
}
